class Xavier_Rafael_OG41_Wurfweite
{
  public static void main(String[] args) 
  {
    double Bogenmass = 2 * (20 * Math.PI / 180);
    double sin = Math.sin(Bogenmass);
    double Distanz = ((1750 * 1750) * sin) / 9.81;
    System.out.println("Die Distanz ist: " + Distanz + "m. \nWim Pel hat es richtig gesagt!");
  }
}
